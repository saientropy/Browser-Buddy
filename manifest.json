{
    "manifest_version": 2,
    "name": "Text Selector Q&A",
    "version": "1.0",
    "description": "Ask follow-up questions about selected text using an open-source AI model.",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["transformers.min.js", "content.js"],
        "css": ["style.css"]
      }
    ],
    "permissions": ["activeTab"]
  }
  